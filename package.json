{
  "name": "athro-federation",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npx turbo run dev",
    "build": "npx turbo run build",
    "lint": "npx turbo run lint",
    "test": "npx turbo run test",
    "clean": "npx turbo run clean",
    "build:dashboard": "npx turbo build --filter=athro-dashboard...",
    "build:dashboard-explicit": "npx turbo build --filter=@athro/shared-types --filter=@athro/shared-services --filter=@athro/shared-ui --filter=@athro/shared-athros --filter=athro-dashboard",
    "build:workspace": "npm run build --workspace=@athro/shared-services && npm run build --workspace=@athro/shared-ui && npm run build --workspace=@athro/shared-types && npm run build --workspace=@athro/shared-athros && npm run build --workspace=athro-workspace-2",
    "build:dashboard-complete": "npx turbo build --filter=@athro/shared-types --filter=@athro/shared-services --filter=@athro/shared-ui --filter=@athro/shared-athros --filter=athro-workspace-2 --filter=athro-dashboard"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/moment": "^2.11.29",
    "@types/react-big-calendar": "^1.16.2",
    "moment": "^2.30.1",
    "react-big-calendar": "^1.19.2",
    "react-icons": "^5.5.0"
  }
}
